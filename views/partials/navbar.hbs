<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css
">
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.0/css/all.min.css"/>
<link rel="stylesheet" href="/css/style.css">

<nav class="navbar navbar-expand-lg bg-body-tertiary py-2 border-bottom">
    <div class="container">
        
        <a class="navbar-brand fw-bold" href="/">
        <img src=" {{data.brandIconUrl}}" width="80px" height="50px" alt="brand image">
        <span class="pt-2">{{data.brandName}}</span>
        </a>
        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent"
            aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav ms-auto mb-2 mb-lg-0">
                {{#each data.links}}
                <li class="nav-item">
                    <a class="nav-link active" aria-current="page" href="{{this.url}}">{{this.label}}</a>
                </li>
                {{/each}}
            </ul>
        </div>
    </div>
</nav>